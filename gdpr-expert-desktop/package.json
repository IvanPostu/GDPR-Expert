{
  "name": "gdpr-expert-desktopp",
  "version": "0.0.1",
  "build": {
    "productName": "GDPR Expert",
    "appId": "university.project"
  },
  "description": "GDPR Expert.",
  "main": "build/electron/electron.js",
  "scripts": {
    "dev": "./node_modules/.bin/env-cmd -f ./.env-cmdrc.json -e development webpack-dev-server --config configs/webpack.app.js",
    "electron-build": "webpack --config configs/webpack.electron.js",
    "app-build": "./node_modules/.bin/env-cmd -e production webpack --config configs/webpack.app.js",
    "dev-server": "npm run electron-build && ./node_modules/.bin/env-cmd -f ./.env-cmdrc.json -e development webpack-dev-server --config configs/webpack.app.js",
    "start": "./node_modules/.bin/env-cmd -f ./.env-cmdrc.json -e development electron ./build/electron/electron.js --enable-loging --remote-debugging-port=9222",
    "package:linux": "npm run electron-build && npm run app-build && electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "test": "jest"
  },
  "author": "IvanPostu",
  "license": "ISC",
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-icons": "^3.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "autoprefixer": "^10.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.0",
    "cssnano": "^4.1.10",
    "electron": "^10.1.4",
    "electron-packager": "^15.1.0",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.0",
    "mini-css-extract-plugin": "^1.1.1",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.1.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.6",
    "typescript": "^4.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
